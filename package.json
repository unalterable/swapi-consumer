{
  "name": "swapi-consumer",
  "version": "1.0.0",
  "description": "Swapi Consumer Tech Test",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-assets:prod && npm run server",
    "dev": "concurrently -k -p '{time} {name}:' -t 'HH:ss' -n 'Assets,Server' -c 'green,magenta' 'npm run build-assets:watch' 'npm run server:watch'",
    "stub-data": "node scripts/stub-data.js",
    "test": "npm run build-assets:prod && mocha test/**/*.test.js --require test/helpers/setup --timeout=60000 --exit",
    "server": "node src/server/index.js",
    "server:watch": "nodemon -w src/ -e js,jsx src/server/index.js",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint-fix": "eslint --ext .js,.jsx --ignore-path .gitignore . --fix",
    "build-assets": "webpack --config webpack.dev.js",
    "build-assets:prod": "webpack --config webpack.prod.js",
    "build-assets:watch": "webpack --config webpack.dev.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^3.8.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.28",
    "babel-loader": "^8.0.4",
    "config": "^3.0.1",
    "debounce": "^1.2.0",
    "express": "^4.16.4",
    "jss": "^9.8.7",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "webdriverio": "^5.4.17",
    "webpack": "^4.28.2"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "docker-starter": "^1.0.1",
    "eslint": "^5.11.1",
    "eslint-plugin-react": "^7.12.0",
    "mocha": "^5.2.0",
    "mocha-prepare": "^0.1.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "webpack-cli": "^3.1.2"
  }
}
